    (local $p_student i32)
    (local $student i32)
    (local $@temp_i32 i32)
    (local.set $p_student (call $malloc (i32.const 4)))
    (local.set $@temp_i32 (call $makeStudent (i32.const 1080) (local.get $p_student)))
    (i32.store align=4 (local.get $@temp_i32) (i32.mul (i32.load align=4 (local.get $@temp_i32)) (i32.const 2)))
    (local.set $student (i32.load align=4 (local.get $p_student)))
    (call $print_student (local.get $student))
    (i32.store align=4 (local.get $student) (i32.sub (i32.load align=4 (local.get $student)) (i32.const 1)))
    (call $print_student (i32.load align=4 (local.get $p_student)))
    (i32.const 0))
  (func $malloc (param $size i32) (result i32)
    (local $res i32)
    (local $pages i32)
    (if (i32.lt_s (global.get $__available_size) (i32.const 0))
