  (data (i32.const 1024) "Calling makeStudent()\0A\00Student name is %s, access = %d\0A\00Vasya\00")
  (func $makeStudent (param $name i32) (param $p_res i32) (result i32)
    (local $@stack_entry i32)
    (local $res i32)
    (local.set $@stack_entry (global.get $@stack))
    (call $printf (i32.const 1024) (global.get $@stack))
    (local.set $res (call $malloc (i32.const 8)))
    (i32.store align=4 (i32.add (local.get $res) (i32.const 4)) (local.get $name))
    (i32.store align=4 (local.get $res) (i32.const 11))
    (i32.store align=4 (local.get $p_res) (local.get $res))
    (global.set $@stack (local.get $@stack_entry))
    (local.get $res))
  (func $print_student (param $student i32)
    (local $@stack_entry i32)
    (local.set $@stack_entry (global.get $@stack))
    (i64.store align=8 (global.get $@stack) (i64.extend_i32_s (i32.load align=4 (i32.add (local.get $student) (i32.const 4)))))
    (global.set $@stack (i32.add (global.get $@stack) (i32.const 8)))
    (i64.store align=8 (global.get $@stack) (i64.extend_i32_s (i32.load align=4 (local.get $student))))
    (global.set $@stack (i32.sub (global.get $@stack) (i32.const 8)))
    (call $printf (i32.const 1047) (global.get $@stack))
    (global.set $@stack (local.get $@stack_entry)))
